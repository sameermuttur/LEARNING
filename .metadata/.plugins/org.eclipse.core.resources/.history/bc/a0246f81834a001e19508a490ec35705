package com.kodnest.javapatterns.level3;

public class pY {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=10;
		for(int i=0;i<=n;i++)
		{
		for(int j=0;j<=n;j++)
		{
			if( (i<=5 && i==j) || (i<=5 && j==n-i) || (i>5 && j==n/2))
			{
			System.out.print("* ");
			}
			else
			{
				System.out.print("  ");
			}
		}
		System.out.println();
		}

	}

}
