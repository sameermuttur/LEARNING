package com.kodnest.javapatterns.level3;

public class pattern_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=11;
		for(int i=0;i<=10;i++)
		{
		for(int j=0;j<=10;j++)
		{
			if(i==n/2 || j==n/2)
			{
			System.out.print("* ");
			}
			else
			{
				System.out.print("  ");
			}
		}
		System.out.println();
		}

	}

}
