package com.kodnest.javapatterns.level3;

public class pS {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=10;
		for(int i=0;i<=n;i++)
		{
		for(int j=0;j<=n;j++)
		{
			if((i==0 && j!=0 && j!=n) || (j==n && i==1 || j==n && i==2) || (i==n/2 && j!=0 && j!=n)||(j==0 && i!=0 && i<n/2)
					||(j==n && i>n/2 && i!=n)||(i==n && j!=0 && j!=n))  
			{
			System.out.print("* ");
			}
			else
			{
				System.out.print("  ");
			}
		}
		System.out.println();
		}
	}

}
